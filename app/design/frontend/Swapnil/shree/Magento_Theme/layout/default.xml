<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="logo">
            <arguments>
                <argument name="logo_img_width" xsi:type="number">160</argument>
                <argument name="logo_img_height" xsi:type="number">65</argument>
            </arguments>
        </referenceBlock>

        <move element="top.search" destination="header-wrapper" before="minicart"/>
        <referenceContainer name="header.panel">
            <block class="Magento\Cms\Block\Block" name="phone_number" htmlClass="phone-number">
                <arguments>
                    <argument name="block_id" xsi:type="string">phone_number</argument>
                </arguments>
            </block>
        </referenceContainer>

    </body>
</page>
